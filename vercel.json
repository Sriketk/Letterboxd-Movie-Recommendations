{
    "rewrites": [
        { "source": "/(.*)", "destination": "/api/main_with_swagger.py" }
    ],
    "functions": {
        "api/**": {
            "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
    },
    "build": {
        "env": {
            "PYTHONPATH": "."
        }
    }
}
